<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MOBWEB_TEST.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MOBWEB_TEST"
    Title="MOBWEB_TEST">

    <ShellContent
        Title="Entry"
        ContentTemplate="{DataTemplate local:Screens.EntryScreen}"
        Route="EntryScreen" />
    <ShellContent 
        Title="Home"
        Route="HomeScreen"
        ContentTemplate="{DataTemplate local:Screens.HomeScreen}" />
    <ShellContent
        Title="DataEntry"
        Route="DataEntryScreen"
        ContentTemplate="{DataTemplate local:Screens.DataEntryScreen}" />
    <ShellContent
        Title="Profile"
        Route="ProfileScreen"
        ContentTemplate="{DataTemplate local:Screens.ProfileScreen}" />
    <ShellContent
        Title="DataUpload"
        Route="DataUploadScreen"
        ContentTemplate="{DataTemplate local:Screens.DataUploadScreen}" />
    <ShellContent
        Title="MesicSubsystem"
        Route="MesicSubsystemScreen"
        ContentTemplate="{DataTemplate local:Screens.DataEntrySubsystems.MesicSubsystemScreen}" />
    <ShellContent
        Title="PlotEntry"
        Route="PlotEntryScreen"
        ContentTemplate="{DataTemplate local:Screens.DataEntrySubsystems.PlotEntryScreen}" />
</Shell>
